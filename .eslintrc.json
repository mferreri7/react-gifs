{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "space-before-function-paren": 0,
    "react/prefer-stateless-function": "off",
    "react/destructuring-assignment": [true, "always", { "ignoreClassFields": true }],
    "array-bracket-spacing": "off"
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint"
}
